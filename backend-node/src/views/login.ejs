<!DOCTYPE html>
<html lang="en">
  <head>
    <%- include("./partials/head") %>
    <title>Login</title>
  </head>
  <body>
    <%- include("./partials/scripts") %> <%- include("./partials/nav") %>
    <div class="container mt-4">
      <form class="row g-10" action="/api/login" method="post">
        <div class="col-md-12">
          <label for="email" class="form-label">Email</label>
          <input
            type="email"
            class="form-control"
            id="email"
            placeholder="example@gmail.com"
            name="email"
          />
        </div>
        <div class="col-md-12">
          <label for="password" class="form-label">Password</label>
          <input
            type="password"
            class="form-control"
            id="password"
            name="password"
          />
        </div>
        <div class="container mt-2"></div>
        <div class="col-12">
          <button type="submit" class="btn btn-primary">login</button>
        </div>
        <div></div>
      </form>
    </div>
  </body>
</html>
